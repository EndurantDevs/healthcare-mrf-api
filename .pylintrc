[MASTER]
ignore=alembic,restore,tests,__test__
load-plugins=
jobs=1
extension-pkg-whitelist=

[MESSAGES CONTROL]
disable=
    missing-docstring,
    invalid-name,
    too-few-public-methods,
    unused-argument,
    unused-import,
    no-member,
    not-callable,
    protected-access,
    broad-exception-caught,
    consider-using-enumerate,
    consider-using-f-string,
    too-many-return-statements,
    too-many-branches,
    too-many-statements,
    too-many-locals,
    too-many-lines,
    line-too-long,
    duplicate-code,
    assignment-from-no-return,
    trailing-whitespace,
    redefined-outer-name,
    import-outside-toplevel,
    no-self-argument,
    wrong-import-position

[REPORTS]
output-format = text

[FORMAT]
max-line-length = 120
